<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" version="1.0">
  <info>
    <title>APA Custom Full with Reports - Updated et al</title>
    <id>http://www.zotero.org/styles/apa-custom-full-reports-updated</id>
    <link href="http://www.zotero.org/styles/apa-custom-full-reports-updated" rel="self"/>
    <author>
      <name>Custom generated by ChatGPT</name>
    </author>
    <category citation-format="author-date"/>
    <updated>2025-08-12T12:30:00+00:00</updated>
  </info>

  <macro name="author">
    <names variable="author" delimiter=", " and="text" et-al-min="3" et-al-use-first="1">
      <name font-weight="bold" and="text"/>
      <et-al font-style="italic" font-weight="bold"/>
    </names>
  </macro>

  <macro name="issued">
    <group delimiter="">
      <date variable="issued" prefix=", " suffix="">
        <date-part name="year" font-weight="bold"/>
      </date>
    </group>
  </macro>

  <macro name="issued-dot-dash">
    <group delimiter="">
      <date variable="issued" prefix=", " suffix=".– ">
        <date-part name="year" font-weight="bold"/>
      </date>
    </group>
  </macro>

  <macro name="title">
    <choose>
      <if type="book thesis report" match="any">
        <text variable="title" font-style="italic"/>
      </if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>

  <macro name="container-title">
    <choose>
      <if type="article-journal paper-conference chapter paper-conference report" match="any">
        <text variable="container-title" font-style="italic"/>
      </if>
      <else>
        <text variable="container-title"/>
      </else>
    </choose>
  </macro>

  <macro name="volume-pages">
    <choose>
      <if type="article-journal">
        <group delimiter=": ">
          <text variable="volume"/>
          <text variable="page"/>
        </group>
      </if>
      <if type="paper-conference">
        <group delimiter=", pp. ">
          <text variable="volume"/>
          <text variable="page"/>
        </group>
      </if>
      <if type="report thesis book chapter">
        <group delimiter=", pp. ">
          <text variable="page"/>
        </group>
      </if>
      <else>
        <text variable="page"/>
      </else>
    </choose>
  </macro>

  <macro name="publisher-place">
    <group delimiter=", ">
      <text variable="publisher"/>
      <text variable="publisher-place"/>
    </group>
  </macro>

  <bibliography et-al-min="3" et-al-use-first="1" line-spacing="1.5" hanging-indent="true">
    <layout>
      <choose>
        <if type="article-journal">
          <text macro="author"/>
          <text macro="issued" prefix=" "/>
          <text value="– " />
          <text macro="title" suffix=". "/>
          <text macro="container-title" suffix=", "/>
          <text macro="volume-pages" suffix="."/>
        </if>

        <if type="paper-conference">
          <text macro="author"/>
          <text macro="issued-dot-dash" prefix=" "/>
          <text macro="title" suffix=". "/>
          <text macro="container-title" prefix="In " suffix=", "/>
          <text variable="event" suffix=", "/>
          <text variable="event-place" suffix=", "/>
          <text macro="volume-pages" prefix="pp. " suffix="."/>
        </if>

        <if type="book">
          <text macro="author"/>
          <text macro="issued-dot-dash" prefix=" "/>
          <text macro="title" suffix=". "/>
          <text macro="publisher-place" suffix=", "/>
          <text macro="volume-pages" prefix="" suffix=" p."/>
        </if>

        <if type="chapter">
          <text macro="author"/>
          <text macro="issued-dot-dash" prefix=" "/>
          <text macro="title" suffix=". "/>
          <text macro="container-title" prefix="In: " suffix=". "/>
          <text variable="collection-title" suffix=", "/>
          <text macro="publisher-place" suffix=", "/>
          <text macro="volume-pages" prefix="pp. " suffix="."/>
        </if>

        <if type="thesis">
          <text macro="author"/>
          <text macro="issued-dot-dash" prefix=" "/>
          <text macro="title" suffix=". "/>
          <text variable="genre" suffix=", "/>
          <text macro="publisher-place" suffix=", "/>
          <text macro="volume-pages" prefix="" suffix=" p."/>
        </if>

        <if type="report">
          <text macro="author"/>
          <text macro="issued-dot-dash" prefix=" "/>
          <text macro="title" suffix=". "/>
          <text variable="collection-title" suffix=", "/>
          <text macro="publisher-place" suffix=", "/>
          <text macro="volume-pages" prefix="" suffix=" p."/>
        </if>

        <else>
          <text macro="author"/>
          <text macro="issued" prefix=" "/>
          <text macro="title" suffix=". "/>
        </else>
      </choose>
    </layout>
  </bibliography>

  <citation>
    <layout delimiter=", ">
      <names variable="author" delimiter=", " and="text" et-al-min="3" et-al-use-first="1">
        <name font-weight="bold" and="text"/>
        <et-al font-style="italic" font-weight="bold"/>
      </names>
      <group prefix=" (" suffix=")">
        <date variable="issued">
          <date-part name="year" font-weight="bold"/>
        </date>
      </group>
    </layout>
  </citation>
</style>